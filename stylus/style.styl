.divparallax
     position relative
     height 520px
     width 100%
     overflow hidden
     
.parallax
     z-index 1
     position absolute 
     width 100%
     background-size: 100% !important    
     height 520px
     background-position center center !important
     background-attachment fixed !important
     background-repeat no-repeat !important
        
.color-green
     z-index 2
     height 520px
     position absolute
     width 100%
     background-color #1f3622
     opacity 0.6
     
.divparallax h3  
     z-index 3
     top 50px
     position absolute
     color #fff 
     
    
.divparallax img
     z-index 3  
     height 220px
     width 330px 
     top 130px
     position absolute      
    
         
